        	int nxt[][]=new int[n+1][26];
        	for(int i=0;i<26;i++) {
        		nxt[n][i]=n;
        	}
        	for(int i=n-1;i>=0;i--) {
        		for(int j=0;j<26;j++) {
        			if(s.charAt(i)-'a'==j) {
        				nxt[i][s.charAt(i)-'a']=i;
        			}else {
        				nxt[i][j]=nxt[i+1][j];
        			}
        		}
        	}
nxt[i][c]  =  the first position â‰¥ i where character c occurs  (or n if c never appears again)
